            <Image url="@../../../pics/sign-up%20(1).png" />
         </image>
      </ImageView>
      <Button fx:id="back" cache="true" layoutX="659.0" layoutY="421.0" mnemonicParsing="false" onMouseClicked="#back" prefHeight="31.0" prefWidth="94.0" text="Return">
